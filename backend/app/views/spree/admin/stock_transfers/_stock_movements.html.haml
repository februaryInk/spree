#source-movements-table.twelve.columns.alpha
  %table.table.sortable
    %thead{"data-hook" => "bulk_variants_header"}
      %tr
        %th= Spree.t('variant')
        %th= Spree.t('sku')
        %th= Spree.t('quantity')
        %th= Spree.t('count_on_hand')
    %tbody#bulk_variants_tbody
      - stock_movements.each do |movement|
        %tr
          %td= movement.stock_item.variant.name
          %td= movement.stock_item.variant.sku
          %td= movement.quantity
          %td= movement.stock_item.count_on_hand
