= render partial: 'spree/admin/shared/error_messages', locals: { target: @collection }
- content_for :page_title do
  = link_to Spree.t(:collections), spree.admin_collections_url
  \/
  = Spree.t(:new_collection)
= form_for [:admin, @collection] do |f|
  %fieldset{"data-hook" => 'new_collection'}
    = render partial: 'form', locals: { f: f }
    = render partial: 'spree/admin/shared/new_resource_links'
