- content_for :page_title do
  = link_to Spree.t(:collections), spree.admin_collections_url
  \/
  = @collection.name
= render partial: 'spree/admin/shared/error_messages', locals: { target: @collection }
= form_for [:admin, @collection] do |f|
  %fieldset
    = render partial: 'form', locals: { f: f }
    = render partial: 'spree/admin/shared/edit_resource_links'
